{
  "name": "@next-trpc-deno-example/cdk",
  "version": "0.1.0",
  "bin": {
    "api": "bin/api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format-check": "biome format bin && biome format lib",
