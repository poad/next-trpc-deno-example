  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@types/node": "^20.9.3",
    "aws-cdk": "^2.110.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.1",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.4"
  }
